        <div class="overlay"></div>
        
        <!-- Sidebar -->
        <nav class="navbar navbar-fixed-top" id="sidebar-wrapper" role="navigation">
            <ul class="nav sidebar-nav">
                <%= link_to raw("<div class='company_name' style='height:80px'></div>"), {controller: "home", action: "index"} %>
                <li>
                    <a class="scroll_button products_button"><i class='sub_icon fa fa-fw fa-folder'></i>Products</a>
                </li>
                <li>
                    <a class="scroll_button distribution_button"><i class='sub_icon fa fa-fw fa-file-o'></i>Distribution</a>
                </li>
                <li>
                    <a class="scroll_button about_us_button"><i class='sub_icon fa fa-fw fa-plus'></i>About Us</a>
                </li>
                <li>
                    <a class="scroll_button contact_us_button"><i class='sub_icon fa fa-fw fa-cog'></i>Contact Us</a>
                </li>
            </ul>
        </nav>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
          <button type="button" class="hamburger is-closed animated fadeInLeft" data-toggle="offcanvas">
